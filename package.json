{
  "name": "website-3",
  "private": true,
  "scripts": {
    "clean": "turbo clean && git clean -xdf node_modules",
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types",
    "update-common-packages": "pnpm update fumadocs-core fumadocs-mdx fumadocs-ui next tailwindcss tw-animate-css react react-dom lucide-react better-auth @tailwindcss/postcss @types/mdx postcss kysely @types/react @types/react-dom zod tailwind-merge @tanstack/react-router @tanstack/react-start --recursive --latest",
    "web:dev": "turbo run dev --filter=web",
    "web:build": "turbo run build --filter=web",
    "web:serve": "turbo run serve --filter=web",
    "tanstack:dev": "turbo run dev --filter=tanstack",
    "tanstack:build": "turbo run build --filter=tanstack",
    "tanstack:start": "turbo run start --filter=tanstack",
    "tanstack:serve": "turbo run serve --filter=tanstack",
    "tan-blog:dev": "turbo run dev --filter=tan-blog",
    "tan-blog:build": "turbo run build --filter=tan-blog",
    "tan-blog:start": "turbo run start --filter=tan-blog",
    "tan-blog:serve": "turbo run serve --filter=tan-blog"
  },
  "devDependencies": {
    "prettier": "^3.5.3",
    "turbo": "^2.5.0",
    "typescript": "5.8.2"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18"
  },
  "pnpm": {
    "overrides": {
      "esbuild": "0.25.5"
    }
  }
}
